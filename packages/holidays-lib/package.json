{
  "name": "@jbouduin/holidays-lib",
  "version": "0.0.1-alpha.0",
  "description": "Get World-Wide Holidays",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "clear-assets": "rimraf ",
    "copy-old:assets": "cpx src/assets/**/*.json lib/assets",
    "copy:assets": "copyfiles \"src/assets/**/*.json\" lib/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbouduin/holiday.git"
  },
  "keywords": [
    "Holidays",
    "Banking Holidays",
    "Holiday calculation"
  ],
  "author": "Johan Bouduin",
  "license": "MIT"
}
